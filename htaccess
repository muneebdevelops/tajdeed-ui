
# php -- BEGIN cPanel-generated handler, do not edit
# This domain inherits the “PHP” package.
# php -- END cPanel-generated handler, do not edit
RewriteEngine On
RewriteRule ^fertilizer\.html$ http://www.tejdeed.com/fertilizer [L,R=301]
RewriteRule ^index\.html$ http://www.tejdeed.com/home [L,R=301]
RewriteRule ^plastic\.html$ http://www.tejdeed.com/plastic [L,R=301]
RewriteRule ^gas\.html$ http://www.tejdeed.com/gas [L,R=301]
RewriteRule ^biomass\.html$ http://www.tejdeed.com/biomass [L,R=301]


# Enable caching for images, fonts, and PDFs
<IfModule mod_expires.c>
  ExpiresActive On
  ExpiresByType image/jpg "access plus 1 year"
  ExpiresByType image/jpeg "access plus 1 year"
  ExpiresByType image/gif "access plus 1 year"
  ExpiresByType image/png "access plus 1 year"
  ExpiresByType image/webp "access plus 1 year"
  ExpiresByType font/ttf "access plus 1 year"
  ExpiresByType font/woff "access plus 1 year"
  ExpiresByType font/woff2 "access plus 1 year"
  ExpiresByType application/pdf "access plus 1 month"
  ExpiresDefault "access plus 2 days"
</IfModule>

# Enable gzip compression for HTML
<IfModule mod_deflate.c>
  AddOutputFilterByType DEFLATE text/html
</IfModule>

